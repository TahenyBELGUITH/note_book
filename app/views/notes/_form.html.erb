<%= form_with(model: note) do |form|%>
<div>
  <%= form.label :title %>
  <%= form.text_field :title %>
</div><br>

<div>
  <%= form.label :content %>
  <%= form.rich_text_area :content %>
</div>


<div>
  <%= form.hidden_field :user_id, value: current_user.id %>
</div><br>


<div>
  <%= form.submit%>
</div>


<% end %>